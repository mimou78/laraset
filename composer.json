{
    "version": "0.4.1",
    "name": "khofaai/laraset",
    "authors": [
        {
            "name": "Anas El Alami",
            "email": "anaselalamikh@gmail.com"
        }
    ],
    "license": "MIT",
    "description": "Module manager",
    "keywords": ["laravel", "laraset", "php", "package", "module manager", "modular system", "laravel preset","vuejs","vue"],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "illuminate/console" : "~5.5",
        "illuminate/support" : "~5.5",
        "illuminate/database" : "~5.5",
        "illuminate/http" : "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Khofaai\\Laraset\\":"src/"
        },
        "files": [
            "src/helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khofaai\\Laraset\\LarasetServiceProvider",
                "Khofaai\\Laraset\\core\\Providers\\RouteServiceProvider"
            ]
        }
    }
}
